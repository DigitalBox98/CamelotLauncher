<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:CamelotLauncher"
             x:Class="CamelotLauncher.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
    </Application.Styles>

  <NativeMenu.Menu>
    <NativeMenu>
        <NativeMenuItem Header="CamelotLauncher"/>
    </NativeMenu>
  </NativeMenu.Menu>


</Application>
